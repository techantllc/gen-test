{ 
    "title":"a Pet",
    "required":[ 
       "name",
       "photoUrls"
    ],
    "type":"object",
    "properties":{ 
       "id":{ 
          "type":"integer",
          "format":"int64"
       },
       "category":{ 
          "title":"Pet category",
          "type":"object",
          "properties":{ 
             "id":{ 
                "type":"integer",
                "format":"int64"
             },
             "name":{ 
                "type":"string"
             }
          },
          "description":"A category for a pet",
          "example":{ 
             "name":"name",
             "id":6
          },
          "xml":{ 
             "name":"Category"
          }
       },
       "name":{ 
          "type":"string",
          "example":"doggie"
       },
       "photoUrls":{ 
          "type":"array",
          "xml":{ 
             "name":"photoUrl",
             "wrapped":true
          },
          "items":{ 
             "type":"string"
          }
       },
       "tags":{ 
          "type":"array",
          "xml":{ 
             "name":"tag",
             "wrapped":true
          },
          "items":{ 
             "title":"Pet Tag",
             "type":"object",
             "properties":{ 
                "id":{ 
                   "type":"integer",
                   "format":"int64"
                },
                "name":{ 
                   "type":"string"
                },
                "status":{ 
                  "type":"string",
                  "description":"pet status in the store",
                  "enum":[ 
                     "available",
                     "pending",
                     "sold"
                  ]
               }
             },
             "description":"A tag for a pet",
             "example":{ 
                "name":"name",
                "id":1
             },
             "xml":{ 
                "name":"Tag"
             }
          }
       },
       "status":{ 
          "type":"string",
          "description":"pet status in the store",
          "enum":[ 
             "available",
             "pending",
             "sold"
          ]
       }
    },
    "description":"A pet for sale in the pet store",
    "example":{ 
       "photoUrls":[ 
          "photoUrls",
          "photoUrls"
       ],
       "name":"doggie",
       "id":0,
       "category":{ 
          "name":"name",
          "id":6
       },
       "tags":[ 
          { 
             "name":"name",
             "id":1
          },
          { 
             "name":"name",
             "id":1
          }
       ],
       "status":"available"
    },
    "xml":{ 
       "name":"Pet"
    }
 }